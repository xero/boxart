<ImageComposition xsi:noNamespaceSchemaLocation="https://www.skraper.net/ImageComposition.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Information ShortName="super garlicboy" LongName="super garlicboy" Description="3DBox + cart + screenshot + wheel" Author="xero" />
  <Viewport Color="#00FFFFFF" Width="640" Height="480" />
  <Drawings>
    <Item Type="Screenshot">
      <Display X="21.5%" Y="68%" Antialiasing="None" Width="45%" Height="45%" Anchor="BottomHCenter" />
      <Fallback Type="ScreenshotTitle">
        <Fallback Type="SystemWallPaper">
          <Children Reference="Parent">
            <Item Type="Text" Text="No Screenshot!" TextColor="#FFFFFFFF" FontFamilly="Arial" FontStyle="Bold Italic" />
          </Children>
        </Fallback>
      </Fallback>
    </Item>
    <Item Type="NoResource">
      <Children>
        <Item IfOrientation="IsPortrait" Type="Box3D">
					<Display X="0%" Y="58%" Antialiasing="High" Transparency="0.9" Width="20%" Height="26%" Anchor="TopLeft" />
          <Children>
            <Item Type="Support">
							<Display X="9%" Y="65%" Antialiasing="High" Transparency="0.9" Width="12%" Height="23%" Anchor="TopLeft" />
            </Item>
          </Children>
        </Item>
        <Item IfOrientation="IsLandscape" Type="Box3D">
					<Display X="0%" Y="62%" Antialiasing="High" Transparency="0.9" Width="22%" Height="28%" Anchor="TopLeft" />
          <Children>
            <Item Type="Support">
							<Display X="19%" Y="64%" Antialiasing="High" Transparency="0.9" Width="12%" Height="23%" Anchor="TopLeft" />
            </Item>
          </Children>
        </Item>
      </Children>
    </Item>
    <Item Type="Wheel">
      <Display X="0%" Y="12%" Width="27%" Height="30%" Anchor="TopLeft" Antialiasing="High" IfSystem="IsNotInTheList:2"/>
      <Fallback Type="Text" Text="%name%"  TextColor="#FFFFFFFF" FontFamilly="Arial" FontStyle="Bold Italic" IfSystem="IsNotInTheList:2"/>
    </Item>
  </Drawings>
</ImageComposition>
